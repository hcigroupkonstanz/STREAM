{
  "name": "stream-server",
  "version": "0.0.1",
  "main": "dist/server/main.js",
  "scripts": {
    "gui-build": "ng build --prod",
    "gui-watch": "ng build --watch",
    "server-watch-ts": "tsc --p src/server -w",
    "server-watch-node": "nodemon --exec \"npm run server-start\" --watch dist/server/*",
    "server-build": "tsc --p src/server",
    "server-start": "node dist/server/main.js",
    "debug": "concurrently -k -p \"[{name}]\" -n \"Angular,Node\" -c \"green.bold,blue.bold\" \"npm run gui-watch\" \"npm run server-watch-ts\"",
    "build": "npm run gui-build && npm run server-build",
    "start": "npm run server-start",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Angular,Server-Compile,Server-Node\" -c \"green.bold,blue.bold,yellow.bold\" \"npm run gui-watch\" \"npm run server-watch-ts\" \"npm run server-watch-node\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.3",
    "@angular/cdk": "~8.1.1",
    "@angular/common": "~8.1.3",
    "@angular/compiler": "~8.1.3",
    "@angular/core": "~8.1.3",
    "@angular/forms": "~8.1.3",
    "@angular/http": "~7.2.0",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "~8.1.3",
    "@angular/platform-browser-dynamic": "~8.1.3",
    "@angular/router": "~8.1.3",
    "better-sqlite3": "^5.2.1",
    "body-parser": "^1.18.3",
    "core-js": "^3.1.4",
    "cors": "^2.8.5",
    "csv-parse": "^4.3.0",
    "d3": "^5.11.0",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "node-abi": "^2.5.1",
    "perfect-scrollbar": "^1.4.0",
    "require-from-string": "^2.0.2",
    "rxjs": "~6.5.2",
    "sass": "^1.22.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "source-map-support": "^0.5.9",
    "tedious": "^6.2.0",
    "zone.js": "~0.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.3",
    "@angular/language-service": "~8.1.3",
    "@types/better-sqlite3": "^5.2.1",
    "@types/body-parser": "^1.17.0",
    "@types/csv-parse": "^1.1.11",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash": "^4.14.119",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.6.8",
    "@types/perfect-scrollbar": "^1.3.0",
    "@types/require-from-string": "^1.2.0",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/tedious": "^4.0.0",
    "codelyzer": "~5.1.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.0.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nodemon": "^1.19.1",
    "protractor": "~5.4.2",
    "ts-node": "^8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.4.5"
  }
}
