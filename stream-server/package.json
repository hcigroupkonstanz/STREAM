{
  "name": "stream-server",
  "version": "0.0.1",
  "main": "dist/server/main.js",
  "scripts": {
    "gui-build": "ng build",
    "gui-watch": "ng build --watch",
    "server-watch-ts": "tsc --p src/server -w",
    "server-watch-node": "nodemon --exec \"npm run server-start\" --watch dist/server/*",
    "server-build": "tsc --p src/server",
    "server-start": "node dist/server/src/server/main.js",
    "debug": "concurrently -k -p \"[{name}]\" -n \"Angular,Node\" -c \"green.bold,blue.bold\" \"npm run gui-watch\" \"npm run server-watch-ts\"",
    "build": "npm run gui-build && npm run server-build",
    "start": "npm run server-start",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Angular,Server-Compile,Server-Node\" -c \"green.bold,blue.bold,yellow.bold\" \"npm run gui-watch\" \"npm run server-watch-ts\" \"npm run server-watch-node\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.0.3",
    "@angular/cdk": "16.0.2",
    "@angular/common": "16.0.3",
    "@angular/compiler": "16.0.3",
    "@angular/core": "16.0.3",
    "@angular/forms": "16.0.3",
    "@angular/material": "16.0.2",
    "@angular/platform-browser": "16.0.3",
    "@angular/platform-browser-dynamic": "16.0.3",
    "@angular/router": "16.0.3",
    "better-sqlite3": "9.2.2",
    "body-parser": "1.18.3",
    "core-js": "3.1.4",
    "cors": "2.8.5",
    "csv-parse": "4.3.0",
    "d3": "5.11.0",
    "express": "4.18.2",
    "lodash": "4.17.11",
    "moment": "2.23.0",
    "node-abi": "2.5.1",
    "perfect-scrollbar": "1.4.0",
    "require-from-string": "2.0.2",
    "rxjs": "7.8.1",
    "sass": "1.22.2",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "source-map-support": "0.5.9",
    "tedious": "6.2.0",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.0.3",
    "@angular-eslint/eslint-plugin": "^16.1.0",
    "@angular/cli": "16.0.3",
    "@angular/compiler-cli": "16.0.3",
    "@angular/language-service": "16.0.3",
    "@types/better-sqlite3": "7.6.8",
    "@types/body-parser": "1.17.0",
    "@types/csv-parse": "1.1.11",
    "@types/lodash": "4.14.119",
    "@types/moment": "2.13.0",
    "@types/node": "12.6.8",
    "@types/perfect-scrollbar": "1.3.0",
    "@types/require-from-string": "1.2.0",
    "@types/socket.io": "2.1.2",
    "@types/socket.io-client": "1.4.32",
    "@types/tedious": "4.0.0",
    "concurrently": "4.1.0",
    "nodemon": "1.19.1",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "5.0.4"
  }
}
